{
  "manifest_version": 3,
  "name": "Netflix 5s Seek",
  "version": "1.0",
  "description": "Changes Netflix arrow key seek from 10s to 5s",
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["*://*.netflix.com/*"]
    }
  ]
}
